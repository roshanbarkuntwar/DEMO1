<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">


<!-- Author: ayushi.jain -->

<struts>
    <!-- Add your configuration elements here -->
    <package name="regular.generateFVU" extends="struts-default" namespace="/">
        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
        </result-types>
        
        <interceptors>
            <interceptor class="interceptors.LoginInterceptor" name="LoginInterceptor" ></interceptor>
            
            <interceptor-stack name="loginStack">
                <interceptor-ref name="LoginInterceptor" />
                <!--<interceptor-ref name="DeducteeIncomeDetailsInterceptor" />-->
                <interceptor-ref name="defaultStack" />
            </interceptor-stack>
            
            <interceptor class="interceptors.ModuleAccessInterceptor" name="moduleAccessInterceptor"   ></interceptor>
            <interceptor-stack name="moduleAccessStack">
                <interceptor-ref name="params"/>
                <interceptor-ref name="moduleAccessInterceptor" />
                <interceptor-ref name="defaultStack" />
            </interceptor-stack>
        </interceptors>
        
        <default-interceptor-ref name="loginStack"/>
        
        <global-results>
            <result name="loginFailed" type="redirect">login</result>
            <result name="login">/login.jsp</result>
            <result name="setassessment" type="redirect">setassessment</result>
            <result name="globalError" type="redirect">globalError</result>
            <result name="accessDenied" type="tiles">globalAccessDenied</result>
            <result name="customError" type="tiles">customError</result>
        </global-results>
        
        <global-exception-mappings>
            <exception-mapping exception="java.lang.Exception" result="globalError" />
        </global-exception-mappings> 
        
        
    
        <action name="tdsGenerateFVU" class="regular.generateFVU.GenerateFVUAction" method="execute">
            <result name="success" type="tiles">tdsGenerateFVU</result>            
            <result name="error" type="redirect">errorStatus</result>            
        </action>
        
        <action name="validateFVUToGenerate" class="regular.generateFVU.SelectDynamicContentDataAsAJAX" method="execute">            
            <result name="success" type="stream">
                <param name="contentType">text/html</param>
                <param name="inputName">inputStream</param>
            </result> 
        </action>
        
        <action name="getErrorMessageAction" class="regular.generateFVU.ErrorMessageAction" method="execute">         
            <result name="success" type="tiles">customError</result>
        </action>
        
        <!--        <action name="tdsGenerateFVUText" class="regular.generateFVU.GenerateFVUTextFileAction" method="execute">
            <result name="success" type="tiles">generate_file</result>
            <result name="input" type="tiles">generate_file</result>
            <result name="error" type="tiles">fvuError</result>
            <result name="limitError" type="redirect">challanErrorPage?action=fvuLimit</result>
        </action>-->
       
        <action name="fvuFileDashboard" class="regular.generateFVU.FVUfileDashboardAction" method="execute">
            <result name="success">/WEB-INF/view/jsp/regular/generateFVU/fileDashboardFVU.jsp</result>
        </action>
        
        <action name="filetListAction" class="regular.generateFVU.GeneratedFileListAction" method="execute"> 
            <result name="success">/WEB-INF/view/jsp/regular/generateFVU/fileListGrid.jsp</result>           
        </action>      
        
        
        <action name="downloadGeneratedFile" class="regular.generateFVU.DownloadGeneratedFile" method="execute"> 
            <result name="success" type="stream">
                <param name="contentType">application/octet-stream</param>
                <param name="inputName">fileInputStream</param>
                <param name="contentDisposition">attachment;filename="${fileNameToDownload}"</param>
                <param name="bufferSize">1024</param>
            </result>
            <result name="samePage" type="redirectAction">
                <param name="actionName">tdsGenerateFVU?ab=c</param>
                <param name="jsessionid">ij123kih7896kirtwc5638Nsdsuybn3456</param>
                <param name="message">${message}</param>
                <param name="generatedFileName">${fname}</param>
                <param name="generatedFileLink">${filePath}</param>
                <param name="fileTypeFound">${fileTypeFound}</param>
            </result>         
            <result name="challanPage" type="redirectAction">
                <param name="actionName">challans</param>               
                <param name="message">${message}</param>
                
            </result>   
            
        </action>
        <action name="fileToDwnldAction" class="regular.generateFVU.GeneratedFilesToDwnldAction" method="execute"> 
            <result name="success">/WEB-INF/view/jsp/regular/generateFVU/filesToDwnld.jsp</result>           
        </action>
        <action name="fvuAjaxAction" class="regular.generateFVU.GenerateFVUFileAjaxAction" method="execute"> 
            <result name="success" type="stream">
                <param name="contentType">text/html</param>
                <param name="inputName">inputStream</param>
            </result>           
            <result name="page_success">/WEB-INF/view/jsp/regular/generateFVU/textFileGrid.jsp</result>           
        </action>
       
        <action name="runBatchFileAction" class="regular.generateFVU.GenerateFVUAction" method="execute"> 
            <result name="success" type="tiles">tdsGenerateFVU</result>           
            <result name="error" type="tiles">fvuError</result>           
            <result name="drive_error" type="redirectAction">
                <param name="actionName">challanErrorPage</param>
                <param name="action">driveError</param>
            </result>           
        </action>
          
        <action name="generateFvuBtnAction" class="regular.generateFVU.GenerateFVUBtnAction" method="execute">            
            <result name="success" type="tiles">generateFvu</result>   
            <result name="fvuGenSuccess" type="redirectAction">
                <param name="actionName">errorStatus</param>
            </result>
            <result name="drive_error" type="tiles">customError</result>
        </action>
        <action name="generateFvuBtnClobAction" class="regular.generateFVU.GenerateFVUBtnClobAction" method="execute">            
            <result name="success" type="tiles">generateFvuClob</result>   
            <result name="fvuGenSuccess" type="redirectAction">
                <param name="actionName">errorStatus</param>
            </result>
            <result name="drive_error" type="tiles">customError</result>
        </action>
        
        <action name="downloadFvuTxtFile" class="regular.generateFVU.GenerateFVUBtnAction" method="execute">
            <result name="success" type="tiles">generateFvu</result>           
            <result name="file_success" type="stream">
                <param name="contentType">text/html</param>
                <param name="inputName">inputStream</param>
            </result>      
            <result name="drive_error" type="tiles">customError</result>       
        </action>
        
        <action name="fvuGenerateAjaxAction" class="regular.generateFVU.GenerateFVUProcedureAction" method="execute">
            <result name="input" type="redirect">tdsGenerateFVU</result>
            <result name="ajax_success" type="stream">
                <param name="contentType">text/html</param>
                <param name="inputName">inputStream</param>
            </result>
        </action>
        
        <action name="downloadFVUZip" class="regular.generateFVU.DownloadFvuRelatedFiles" method="execute">            
            <result name="success"  type="redirectAction">                
                <param name="actionName">downloadUsingPath</param>                
                <param name="fileNamePath">${fileNamePath}</param>               
            </result>          
        </action>
        
        <action name="generateBulkTextFile" class="regular.generateFVU.SelectDynamicContentDataAsAJAX" method="execute"> 
            <result name="input" type="redirect">errorStatus</result>  
            <result name="error" type="redirect">errorStatus</result>  
            <result name="bulk_gen_init"  type="stream">                
                <param name="contentType">text/html</param>               
                <param name="inputName">inputStream</param>                
            </result>          
        </action>   
             
        <action name="getProcessLogAction" class="regular.generateFVU.SelectDynamicContentDataAsAJAX" method="execute"> 
            <result name="input" type="redirect">errorStatus</result>  
            <result name="error" type="redirect">errorStatus</result>  
            <result name="log_status_result"  type="stream">                
                <param name="contentType">text/html</param>               
                <param name="inputName">inputStream</param>                
            </result>          
        </action>        
    </package>
</struts>

